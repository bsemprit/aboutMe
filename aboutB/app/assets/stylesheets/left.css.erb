 @font-face { font-family: 'Rex'; src: url(<%= asset_path 'RexBold.otf' %>); } 


  body {
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    font-family: "Poiret One", serif;
    background-color: black;
  }

  main {
    flex: 1 0 auto;
  }

  .background {
  	z-index: 0;
  	width: 100%;
  	position: fixed;
  	height: 100vh;
  	background-image: url('galaxygirl.jpg');
 	background-position: center;
 	background-size: cover;
 	background-attachment: fixed;
 	background-repeat: no-repeat;

  }

  .stars {
  	width: 100%;
  	height: 100vh;
  	position: fixed;
  	z-index: 10;
  }


.navbar-fixed {
	position: absolute !important;
}
    

.first-row {
	margin-bottom: 0 !important;
}

.quote {
	font-family: "Allura", serif;
	position: absolute;
	bottom: 0;
	left: 60vh;
}

.list-holder-left {
	font-size: 20px;
	margin-bottom: 0;
}

.list-holder-left li a {
	color: white;
	font-weight: 600;
}

.list-item-left {
	margin-top: 10%;
}


.top-text-holder-left{
	padding-top: 5%;
}

.profile-left {
	width: 60% !important;
	z-index: 11;
}


.left-top {
	/*background-image: url(<%= asset_path 'blurred-abstract.jpg' %>);
