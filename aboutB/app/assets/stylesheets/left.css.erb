 @font-face { font-family: 'Rex'; src: url(<%= asset_path 'RexBold.otf' %>); } 


  body {
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    font-family: "Poiret One", serif;
    background-color: black;
  }

  main {
    flex: 1 0 auto;
  }

  .background {
  	z-index: 0;
  	width: 100%;
  	height: 100vh;
  	position: absolute;
  	background-color: black;
  	background-image: url('galaxygirl.jpg');
 	background-position: center;
 	background-size: cover;
 	background-attachment: fixed;
 	background-repeat: no-repeat;

  }

  .stars {
  	width: 100%;
  	height: 100vh;
  	position: fixed;
  	z-index: 10;
  }


.navbar-fixed {
	position: absolute !important;
}


.nav-bar {
	position: fixed;
	right: 30px;	
	top: 0;
	z-index: 20;
	width: 150px;
	min-width: 50px;
	height: 100vh;
	/*margin-top: 8%;*/
	margin-bottom: 0;
	border-left: 1px solid purple;
	border-right: 1px solid purple;
	/*Fallback to black*/
	background-color: rgba(0, 0, 0, 0.7);
	
	  /* Safari 5.1, Chrome 10+ */
	  /*background: -webkit-linear-gradient(top,  rgba(85, 103, 237, 0.7), rgba(0, 0, 0, 0.7));*/

	  /* Firefox 3.6+ */
	  background: -moz-linear-gradient(top,  rgba(85, 103, 237, 0.7), rgba(0, 0, 0, 0.7));

	  /* IE 10 */
	  background: -ms-linear-gradient(top,  rgba(85, 103, 237, 0.7), rgba(0, 0, 0, 0.7));

	  /* Opera 11.10+ */
	  background: -o-linear-gradient(top,  rgba(85, 103, 237, 0.7),  rgba(0, 0, 0, 0.7));
	padding-bottom: 20px;
}
    



.list-holder-left {
	font-size: 20px;
	margin-bottom: 0;
	margin-top: 30vh;
	position: fixed;
}

.list-holder-left li a {
	color: white;
	font-weight: 600;
	margin-left: 5px;
}

.list-holder-left li a i {
	font-weight: normal;
	font-size: 15px;
}

.list-item-left {
	margin-top:20px;
}

.first-row{
	margin-bottom: 0 !important;
	position: relative;
	width: 100%;
	height: 100%;
}

.left-top {
	/*background-image: url(<%= asset_path 'blurred-abstract.jpg' %>);
	background-position: center;
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: 100%;
	background-color: #64b5f6;*/
	z-index: 5;
	height: 100vh;
	width: 100%;
	position: relative;
}


.left-side-top {
	margin-bottom: 0;
	height: 100vh;
	position: relative;
}

.quote {
	font-family: "Allura", serif;
	position: relative;
	bottom: 0;
	left: 0;
	z-index: 11;
	margin-left: 2%;
}


.top-text-holder-left{
	padding-top: 5%;
	min-height: 80%;
}

.second-row {
	padding: 0 !important;
	position: relative;
	margin-bottom: 0;
}

.left-resume {
	position: relative;
	background-color: #1C1C1C;
	background-image: url(<%= asset_path 'blurred-abstract.jpg' %>);
	background-position: center ;
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: 100%;
	margin: 0;

}

.parts-resume-left {
	font-family: 'Poiret One';
	font-size: 18px;
	position: relative;
	margin-bottom: 0;
}

.journey-section {
	background-color: black;
	background-image: url(<%= asset_path 'journeymove.jpg' %>);
	background-position: bottom;
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: 100%;
	height: 40vh;
	padding-top: 10vh !important;
}

.ironhack-resume-left {
	background-color: rgba(0,0,0,0.7);
	position: relative;
	z-index: 11;
	height: 300px;
	padding-top: 50px;
}

.strayer-text, .ironhack-text, .broward-text {
	position: relative;
}

.strayer-info, .ironhack-info, .broward-info {
	position: relative;
	display: none;
}

.strayer-hover, .ironhack-hover, .broward-hover {
	position: relative;
	margin-top: 50px;
}

.strayer-arrow, .ironhack-arrow, .broward-arrow{
	width: 30px;
	margin-top: 50px;
}

.strayer-holder, .ironhack-holder, .broward-holder {
	min-height: 100%;
}

.strayer-holder:hover .strayer-hover, .ironhack-holder:hover .ironhack-hover, .broward-holder:hover .broward-hover {
	display: none;
}

.strayer-holder:hover .strayer-info, .ironhack-holder:hover .ironhack-info, .broward-holder:hover .broward-info {
	display: block;
	transition: all .3 ease-in;
}

.learning-top {
	margin-top: 50px;
	margin-bottom: 50px;
}

.work-resume-left {
	background-color: rgba(30, 0, 30, 0.7);
	position: relative;
	z-index: 11;
	
	padding-top: 50px;
}



.down-arrow-holder {
	position: relative;
	padding-top: 20px;
	width: 100%;
}

.down-arrow {
	z-index: 11;
	position: relative;
